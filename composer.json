{
    "name": "ngekoding/lumen-api-query-parser-php5",
    "description": "A query parser for REST-APIs based on laravel's lumen framework",
    "keywords": ["lumen", "query parser", "api"],
    "homepage": "https://github.com/ngekoding/lumen-api-query-parser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nur Muhammad",
            "email": "blog.nurmuhammad@gmail.com",
            "homepage": "https://ngekoding.com",
            "role": "developer"
        }
    ],
    "require": {
    	"php": ">=5.6",
        "laravel/lumen-framework": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.1",
        "squizlabs/php_codesniffer": "3.0.0RC4"
    },
    "autoload": {
        "psr-4": {
            "LumenApiQueryParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
	"scripts": {
		"check": [
		 	"@cs-check",
		 	"@test"
		],
		"cs-check": "./vendor/bin/phpcs --colors",
		"test": "./vendor/bin/phpunit --colors=always"
	},
    "minimum-stability": "stable",
    "prefer-stable": true
}